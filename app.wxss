/**app.wxss**/

@import "./utils/wxParse/wxParse.wxss";

.ellipsis {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
}

.ellipsis-multi-line {
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
}

.ellipsis-line-2 {
  -webkit-line-clamp: 2;
}

.ellipsis-line-3 {
  -webkit-line-clamp: 3;
}

.ellipsis-line-4 {
  -webkit-line-clamp: 4;
}

page {
  height: 100%;
}

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
}
